{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python: Current File",
          "type": "debugpy",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "justMyCode": false,
          "python": "${workspaceFolder}/.venv/bin/python",
          "cwd": "${workspaceFolder}",
          "env": {
              "PYTHONPATH": "${workspaceFolder}"
          }
      }
  ]
}